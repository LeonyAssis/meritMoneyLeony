{
  "id": "defs.json",
  "definitions": {
    "boolean": {
      "type": "number",
      "minimum": 0,
      "maximum": 1
    },
    "positiveInt": {
      "type": "integer",
      "minimum": 1
    },
    "positiveNumber": {
      "type": "number",
      "minimum": 0
    },
    "cpf": {
      "type": "string",
      "minLength": 11,
      "maxLength": 11
    },
    "cnpj": {
      "type": "string",
      "minLength": 14,
      "maxLength": 14
    },
    "date": {
      "type": "string",
      "format": "date"
    },
    "dateTime": {
      "type": [
        "string",
        "null"
      ],
      "pattern": "(\\d{4})-(\\d{2})-(\\d{2}) (\\d{2}):(\\d{2}):(\\d{2})"
    },
    "fullString": {
      "type": "string",
      "minLength": 1,
      "maxLength": 255
    },
    "cellphone": {
      "type": "string",
      "pattern": "^[1-9]{2}9?[0-9]{8}$"
    },
    "telefone": {
      "type": "string",
      "minLength": 8,
      "maxLength": 9
    },
    "number": {
      "type": "string",
      "pattern": "^\\d+$"
    },
    "zipcode": {
      "type": "string",
      "pattern": "^[0-9]{8}$"
    },
    "city": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50
    },
    "state": {
      "type": "string",
      "pattern": "^(?:A[CLPM]|BA|CE|DF|ES|GO|M[ATSG]|P[RBAEI]|R[JNSOR]|S[CEP]|TO)$"
    },
    "complement": {
      "type": [
        "string",
        "null"
      ],
      "minLength": 0,
      "maxLength": 30
    },
    "email": {
      "type": "string",
      "pattern": "[A-Za-z0-9_\\-]+([.][A-Za-z0-9_\\-]+)*@[A-Za-z0-9_]+([-.][A-Za-z0-9_]+)*\\.[A-Za-z0-9_]+"
    },
    "fullName": {
      "type": "string",
      "pattern": "^[ ]*(.+[ ]+)+.+[ ]*$",
      "minLength": 1,
      "maxLength": 255
    },
    "cardPassword": {
      "type": "string",
      "pattern": "^[0-9]{4}$"
    },
    "movimentationToken": {
      "type": "string",
      "pattern": "^([0-9a-f]{32})|([0-9a-f]{40})$"
    },
    "signatureValue": {
      "type": "string",
      "pattern": "^[a-z]{5}$"
    },
    "momentTimeShorthand": {
      "type": "string",
      "pattern": "^[yQMwdhmsms]$"
    },
    "longText": {
      "type": "string"
    },
    "array": {
      "type": "string",
      "pattern": "^\\[([0-9]+(,[0-9]+)*)*\\]"
    }
  }
}
