{
  "name": "merit-money",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "start": "node app.js",
    "start-nodemon": "nodemon app.js",
    "test-coverage": "nyc --reporter=json-summary --reporter=text-summary mocha tests/**/* --recursive",
    "test-coverage-html": "nyc --reporter=html mocha tests/**/* --recursive",
    "lint": "eslint",
    "type-checking": "tsc --noEmit",
    "coverage:badge": "coverage-badge-creator"
  },
  "nyc": {
    "all": true,
    "include": [
      "app/adapters/services/**/*.js",
      "app/interactors/**/*.js",
      "app/services/**/*.js"
    ],
    "exclude": [
      "tests/*",
      "infra/*",
      "**/*types.js"
    ]
  },
  "dependencies": {
    "ajv": "^6.12.6",
    "async": "^2.6.4",
    "async-q": "0.3.1",
    "awilix": "^6.0.0",
    "bcrypt": "^5.0.1",
    "body-parser": "1.18.3",
    "bunyan": "1.8.12",
    "cors": "^2.8.5",
    "cron": "^2.1.0",
    "date-holidays": "^1.5.1",
    "express": "4.17.3",
    "fs-extra": "7.0.1",
    "http-shutdown": "1.2.0",
    "immutable": "3.8.2",
    "install": "0.12.2",
    "jsonwebtoken": "8.5.0",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.3",
    "moment": "^2.29.3",
    "moment-timezone": "^0.5.34",
    "mysql2": "2.3.3",
    "natives": "1.1.6",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "pug": "^3.0.2",
    "q": "1.5.1",
    "re2": "^1.17.4",
    "sequelize": "^5.22.5",
    "sequelize-cli": "^5.5.1",
    "underscore.string": "3.3.5"
  },
  "devDependencies": {
    "del": "4.0.0",
    "dotenv": "^16.0.0",
    "jshint": "2.13.4",
    "jshint-stylish": "2.2.1",
    "mocha": "^6.2.3",
    "nyc": "15.1.0",
    "require-dir": "1.2.0",
    "run-sequence": "2.2.1",
    "should": "13.2.3",
    "should-sinon": "0.0.6",
    "sinon": "7.3.2",
    "yargs": "13.2.2"
  }
}
